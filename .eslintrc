{
  "extends": ["airbnb"],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "import/prefer-default-export": "off",
    "react/prop-types": 0,
    "max-len": [2, 80, 4, {"ignoreUrls": true}],
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "never",
      "imports": "never",
      "exports": "never",
      "functions": "never"
  }]
  }
}
